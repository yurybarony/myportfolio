block companies
  .container-fluid#companies
    section.section-proof.section.bg-white.py-5.section-comp
      .container
        h3.section-title.font-weight-bold.text-center.mb-5 #{sections.companies}
        .logos.row.mb-5
          each comp in companies
            .logo.px-3.col-6.col-md-4.col-lg-3.mr-0.px-md-5.px-lg-4
              a.logo-link(href=`${comp.href}`, target="_blank")
                img.img-fluid.svg-ie-fix(src=`../images/${comp.imgSrc}`)